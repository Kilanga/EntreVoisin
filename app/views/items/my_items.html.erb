<div class="container mt-3">
  <h1>Mes objets</h1>
  <div class="cards-items">
    <% @items.each do |item| %>
      <div class="card-item">
        <%= image_tag(item.picture) %>
        <div class="card-item-infos">
          <h2><%= item.name %></h2>
        </div>
      </div>
    <% end %>
  </div>
</div>
